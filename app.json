{
  "expo": {
    
    "name": "E-LearningTool",
    "slug": "E-LearningTool",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./App/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "requireFullScreen": true,
      "adaptiveIcon": {
        "foregroundImage": "./App/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./App/assets/favicon.png"
    },
    "plugins": [
      [
        
    "expo-font",
        {
          "fonts": [
            "./App/assets/fonts/InriaSans-Regular.ttf",
            "./App/assets/fonts/InriaSans-Bold.ttf",
            "./App/assets/fonts/PaytoneOne-Regular.ttf",
            "./App/assets/fonts/Inter_18pt-Regular.ttf",
            "./App/assets/fonts/Inter_18pt-Bold.ttf"
          ]
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
        
      ]
    ]
  }
}
